package SeeAndSay.animalImpl;

import SeeAndSay.Animal;
import SeeAndSay.Talker;

/**
 * The Careval1 class is used to represent an instance of the animal class.
 */

public class Careval1 extends Animal implements Talker {

    private final String sound;

    /**
     * Constuctor that accepts a single string parameter.
     *
     * @param Name The name of the Careval1 animal
     */

    public Careval1(String Name){
        super(Name);

        this.sound = "Borf.";
    }

    /**
     * Gets the sound of the animal.
     *
     * @return The sound of the animal
     */
    public String getSound(){
        return this.sound;
    }
}
