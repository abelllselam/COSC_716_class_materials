package objectAdventure.world.awolde1;

import objectAdventure.core.room.Room;


public class MyRoom extends Room {
    
    public MyRoom(int roomId, String roomName) {
        super(roomId, roomName);
        this.addItem(new AncientKey());
    }

    
    public static MyRoom newInstance(){
        return new MyRoom(1, "Abel's Secret Chamber");
    }
}
